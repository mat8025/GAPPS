Vers = "1.3"


proc vers2ele( vstr)
{
 int maj = atoi(vstr);
  v = atof(vstr)
 int min = (v*10 - 10*maj);
 elestr = pt(min);
 str ele =" ";
 ele = spat(elestr,",")
//<<"$ele $(typeof(ele))\n";
//<<"$ele";
 return ele;
 
}
//======================

proc Foo()
{
   return 1;
}

  ec = vers2ele(Vers)
<<"$Vers $ec \n"

//<<"$Vers  $(vers2ele(Vers))\n"
a=Foo()
<<"$a $(typeof(a))\n"

<<"$(Foo(1))\n"


