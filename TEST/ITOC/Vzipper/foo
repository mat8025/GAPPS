

I = vgen(INT,10,0,1)

K = vgen(INT,10,10,-1)


<<"%V$I\n"

<<"%V$K\n"


Z = vZipper(I,K)


<<"%V$Z\n"


T = vgen(INT,5,50,-1)

<<"%V$T\n"


P = vZipper(I,T,4)


<<"%V$P\n"


T = vgen(INT,15,50,-1)

<<"%V$T\n"


P = vZipper(I,T,4)


<<"%V$P\n"


F = vgen(FLOAT_,10,0,1)

V = vgen(FLOAT_,15,50,-1)


W =  vZipper(F,V,4)

<<"%V6.2f$W\n"