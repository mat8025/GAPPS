   int a = 1;
   int b = 2;
   
   proc Goo()
   {
     
     plop = 0; 
     
     if (a > 0 ) {
       <<" ($a > 0)  \n"; 
       plop = 1; 
       }
     else {
       <<" NOT ($a > 0 ) \n"; 
       }
     
     
     chkT(plop); 
     
     return plop; 
     
     
     }
   
//==========================//
   
   proc Goo2()
   {
     plop = 0;
     
     int a = 2;
     int b = 3;
     <<"%V $a $b \n"; 
     if ((a > 0) && (b > 2))
       {
       <<" ($a > 0 && $b > 2) \n"; 
       plop = 1;
       }
     else {
       <<" NOT ($a > 0 && $b > 2) \n";
       }
     
     
     chkT(plop); 
     
     return plop; 
   }
