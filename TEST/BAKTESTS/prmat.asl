
// print out 2D mat easily ?

setdebug(0)

opendll("math")

float e[] = {1.1,2.2,3.3,4.4}
<<"%5.3f<${e}> \n\n"

<<"%5.3f<${e}>\n\n"
<<"%5.3f%v<${e}>\n\n"
<<"%v%5.3f<${e}>\n\n"
<<"%5.3f<${e}>\n\n"
<<"%5.3f%v<${e}>\n\n"
<<"%5.3f%v<${e}>\n\n"


<<"<${e}>\n\n"

 E = diag(e)

<<"%v%6.2f $E \n"
//<<" $(Cab(E)) $(Caz(E)) \n"

<<" %{4\n}%6.2f$E \n"

<<"\n %v $E[0][0] \n"
<<"\n %v $E[0][1] \n"
<<"\n %v $E[1][1] \n"
<<"\n %v $E[2][2] \n"
<<"\n %v $E[3][3] \n"

lw = E[0][::]

<<" $lw \n"

lw = E[1][::]

<<" $lw \n"


lw = E[0:1][::]

<<" $lw \n"

<<"\n %v ${E[0][::]} \n"

<<"\n %v ${E[1][::]} \n"

<<"\n %v %{4\n}${E[1:3][::]} \n"

;